# Default ignored files
/shelf/
/workspace.xml
# Editor-based HTTP Client requests
/httpRequests/
# Datasource local storage ignored files
/dataSources/
/dataSources.local.xml

# Java 编译产物（字节码文件）
*.class

# Maven 构建目录（包含编译输出、依赖、打包文件等）
target/

# Gradle 构建目录（包含编译输出、缓存等）
build/
.gradle/

# Eclipse IDE 相关文件
.classpath
.project
.settings/

# IntelliJ IDEA 额外的 Java 相关配置文件
*.iml
.idea/modules.xml
.idea/jarRepositories.xml
.idea/compiler.xml
.idea/encodings.xml
.idea/resources_environment.xml

# Java 打包文件（通常为构建产物，无需提交）
*.jar
*.war
*.ear

# 日志文件
*.log

# 本地配置文件（避免包含敏感信息，如数据库密码等）
*.local.properties
*.private.properties